{
    "include": ["**/*.{js,ts,jsx,tsx}"],
    "exclude": [
        "node_modules",
        "coverage",
        "dist",
        "test",
        "**/*.config.{js,ts}"
    ],
    "reporter": ["text", "html"],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "functions": 80,
    "lines": 80,
    "statements": 80
}
